{
  "seleniumAddress": "http://localhost:4444/wd/hub",
  "allScriptsTimeout": 200000,
  "capabilities": {
    "browserName": "chrome",
    "shardTestFiles": true,
    "maxInstances": 8,
    "marionette": true,
    "acceptInsecureCerts": true,
    "acceptSslCerts": true,
    "chromeOptions": {
      "args": ["--headless", "--no-sandbox", "--window-size=1920,1080"],
      "prefs": {
        "download": {
          "prompt_for_download": false
        }
      }
    }
  },
  "plugins": [
    {
      "package": "protractor-console",
      "logLevels": ["info", "debug", "warning", "severe"]
    }
  ],
  "specs": [
    "specs/*.spec.js"
  ],
  "framework": "jasmine",
  "getPageTimeout": 60000,
  "jasmineNodeOpts": {
    "showColors": true,
    "defaultTimeoutInterval": 1000000,
    "random": false,
    "stopSpecOnExpectationFailure": true
  },

  "params": {
    "apiUrl": " https://localhost:8443/api/4.0",
    "baseUrl": "https://localhost:8443/",
    "login": {
      "username": "admin",
      "password": "twelve12"
    }
  }
}
